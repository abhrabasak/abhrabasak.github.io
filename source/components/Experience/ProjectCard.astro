---
import Icons8 from "@icons/icons8/Icons8.astro";
import type { Project } from "./Project";

const project = Astro.props as Project;
---

<article class="project-card">
  <Icons8 iconName={project.icon} link={project.url} site={project.name} />
  <span class="project-url"><a href={project.url}>{project.name}</a></span>
  <span class="synopsis">{project.synopsis}</span>
  <span class="tech">{project.tech}</span>
  <span class="year">{project.year}</span>
</article>
